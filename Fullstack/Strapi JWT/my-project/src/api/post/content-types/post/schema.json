      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "view": {
      "type": "integer"
    },
    "like": {
      "type": "integer"
    },
    "likedBy": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "likedPosts"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
